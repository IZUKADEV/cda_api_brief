{
  "env": {
    "node": true,
    "es2021": true,
    "vitest/globals": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["vitest"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
